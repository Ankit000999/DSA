//recursion
//printing digits n-1 using recursion.
//(whitout using f(i+1,n) problem 3
//backtrecking.


#include <bits/stdc++.h>
using namespace std;

void f(int i , int n){
    if(i>n){
        return;
    } 
    f(i+1 ,n);
   cout << i << " ";
}

int main()
{
    f(1,5);
    return 0;
}
