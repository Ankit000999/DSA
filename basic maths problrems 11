/* Create a program that takes a student's score as 
 input and prints their grade based on the following criteria:
A: 90-100
B: 80-89
C: 70-79
D: 60-69
E: 0-59*/

#include<bits/stdc++.h>
using namespace std;
int main() {
  int num;
  cin>>num;
  
 if(num<=100 && num >=90) 
{cout <<"A";}

else if(num<=89 && num >=80) 
{cout <<"B";}

else if(num<=79 && num >=70) 
{cout <<"C";}

else if(num<=69 && num >=60) 
{cout <<"D";}

else if (num<=59 && num >=0) 
{cout <<"E";}

else {
    cout<<"invalid input.please a number between 0 to 100 ";
}
return 0;
}